void deBruijn2(void){
  long dbTimer = millis();
  display.clear();
  display.setCursor(0,0);
  display.print(F("De Breijn Attack2"));
  display.setCursor(98,7);
  display.print(F("Stop"));
  int protocol = 1;
  int bitlength = 4107;
  mhz433t.setProtocol(protocol);
  display.setCursor(41,3);
  display.print(F("Transmitting..."));
  //Stop repeats
  mhz433t.setRepeatTransmit(1);
  //Warm up
  for (int i = 0; i < 5; i++){
    mhz433t.send(1111,12);
  }
  // INT TOO LARGE mhz433t.send(F(),bitlength);

  //Restore repeats
  mhz433t.setRepeatTransmit(10);

  display.setCursor(41,3);
  display.print(F("           "));
  display.setCursor(41,3);
  display.print(F("Complete       "));
  display.setCursor(0,5);
  display.print(("Time: "));
  float timeTaken = ((float)(millis()-(float)dbTimer) / 1000);
  display.print(timeTaken);
  display.print(F(" seconds"));
  delay(5000);
  display.clear();
  return;
}

